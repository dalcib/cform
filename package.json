{
  "name": "cform",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "author": "Dalci de Jesus Bagolin",
  "scripts": {
    "start": "run-p start-*",
    "build": "react-scripts build",
    "test": "jest",
    "u": "jest -u",
    "test-cra": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-cra": "react-scripts start",
    "start-ts": "tsc -w",
    "init-npm": "npm link react react-dom react-scripts mobx mobx-react js-plus tslib ts-jest jest",
    "init-types": "npm link @types/react @types/react-dom @types/jest @types/node",
    "install-eslint": "npm install -g eslint-config-react-app eslint babel-eslint eslint-plugin-react eslint-plugin-import eslint-plugin-jsx-a11y eslint-plugin-flowtype eslint-plugin-import",
    "init": "run-s init-*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "react-scripts": "",
    "tslib": "",
    "@types/react": "",
    "@types/react-native": "",
    "@types/react-dom": "",
    "@types/jest": "",
    "@types/node": "",
    "ts-jest": "",
    "eslint-config-react-app": "",
    "jest": ""
  },
  "dependencies": {
    "react": "",
    "react-native-web": "",
    "react-dom": "",
    "mobx": "",
    "mobx-react": "",
    "js-plus": "",
    "ajv": "",
    "empty-schema": "",
    "carbon-ui": ""
  },
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalcib/cform.git"
  },
  "bugs": {
    "url": "https://github.com/dalcib/cform/issues"
  },
  "homepage": "https://github.com/dalcib/cform#readme"
}
